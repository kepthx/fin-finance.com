//- index.pug
extends ../../layout.pug

block title
	title #{__('FAQ')} | Fintech Finance

block content
	.content
		.wrapper
			h1.big #{__('FAQ')}
			if language.code == 'en'
				include langs/en
			else if language.code == 'es'
				include langs/es
			else if language.code == 'ru'
				include langs/ru
			else if language.code == 'de'
				include langs/de
